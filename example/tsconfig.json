{
  "compilerOptions": {
    "experimentalDecorators": true,
    "outDir": "./lib",
    "rootDir": "./src",
    "baseUrl": ".",
    "target": "ES2020",
    "module": "commonjs",
    "declaration": true,
    "esModuleInterop":true,
    "lib": [
      "es6",
      "dom"
    ],
    "paths": {
      "@/*": ["src/*"],
      "koa-msc": ["../src"],
      "koa-msc/*": ["../src/*"]
    },
  },
  "include": ["./src/"]
}
